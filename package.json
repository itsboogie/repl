{
  "name": "repl",
  "version": "0.0.0",
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "scripts": {
    "jest:watch": "npx jest --watchAll",
    "repl": "ts-node --esm index.ts",
    "test": "jest --testFile=index.test.ts --watch"
  },
  "dependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^18.11.9",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  }
}
